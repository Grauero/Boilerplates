{
  "name": "js-template",
  "version": "1.0.0",
  "description": "Basic template for JS apps, includes HMR, Babel, CSS modules",
  "keywords": [
    "JS, Webpack, Babel, HMR"
  ],
  "author": "Grauero",
  "homepage": "https://github.com/Grauero/Webpack-boilerplates",
  "license": "ISC",
  "private": true,
  "sideEffects": [
    "*.scss",
    "*.css",
    "*.png",
    "*.jpg",
    "*.jpeg"
  ],
  "scripts": {
    "build": "webpack --config webpack-configuration/webpack.prod.js",
    "dev": "webpack-dev-server --open --config webpack-configuration/webpack.dev.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
