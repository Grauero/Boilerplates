{
  "name": "javascript-template",
  "version": "1.0.0",
  "description": "Basic template for JavaScript apps, includes HMR, Babel, CSS modules",
  "keywords": [
    "JavaScript, Webpack, Babel, HMR"
  ],
  "author": "Grauero",
  "homepage": "https://github.com/Grauero/Webpack-boilerplates",
  "license": "ISC",
  "private": true,
  "sideEffects": [
    "*.scss",
    "*.css",
    "*.png",
    "*.jpg",
    "*.jpeg"
  ],
  "scripts": {
    "build": "webpack --config webpack-configuration/webpack.prod.js",
    "dev": "webpack-dev-server --open --config webpack-configuration/webpack.dev.js"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
